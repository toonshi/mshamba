{
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "shrink": false,
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "hostname": "internet_identity.localhost",
      "origin": "http://internet_identity.localhost:4943"
    },
    "token_factory": {
      "type": "rust",
      "package": "token_factory",
      "candid": "src/token_factory/token_factory.did"
    },
    "mshamba_backend": {
      "main": "src/mshamba_backend/main.mo",
      "type": "motoko",
      "build": ["--enhanced-orthogonal-persistence"],
      "dependencies": [
        "token_factory"
      ]
    },
    "mshamba_frontend": {
      "type": "assets",
      "source": ["src/mshamba_frontend/dist"],
      "build": "cd src/mshamba_frontend && npm install && npm run build",
      "dependencies": [
        "mshamba_backend"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "--enhanced-orthogonal-persistence",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "declarations": {
    "output": "src/declarations",
    "node_compatibility": true
  }
}
