{
  "canisters": {
    "internet_identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "token_factory": {
      "type": "rust",
      "package": "token_factory",
      "candid": "src/token_factory/token_factory.did"
    },
    "mshamba_backend": {
      "main": "src/mshamba_backend/main.mo",
      "type": "motoko",
      "build": ["--enhanced-orthogonal-persistence"],
      "dependencies": [
        "token_factory",
        "marketplace",
        "hedera_integration"
      ]
    },
    "marketplace": {
      "main": "src/mshamba_backend/marketplace.mo",
      "type": "motoko",
      "build": ["--enhanced-orthogonal-persistence"]
    },
    "hedera_integration": {
      "main": "src/mshamba_backend/hedera_integration.mo",
      "type": "motoko",
      "build": ["--enhanced-orthogonal-persistence"]
    },
    "mshamba_frontend": {
      "type": "assets",
      "source": ["src/mshamba_frontend/dist"],
      "build": "cd src/mshamba_frontend && npm install && npm run build",
      "dependencies": [
        "mshamba_backend"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "--enhanced-orthogonal-persistence",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "declarations": {
    "output": "src/declarations",
    "node_compatibility": true
  }
}
